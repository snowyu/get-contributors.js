{
  "name": "get-contributors",
  "description": "Get contributors from git, and generate a JSON list of contributors or a list base on a user defined template.",
  "version": "0.1.1",
  "homepage": "https://github.com/snowyu/get-contributors.js",
  "preferGlobal": true,
  "bin": {
    "get-contributors": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/snowyu/get-contributors.js.git"
  },
  "files": [
    "README.md",
    "LICENSE-MIT",
    "usage.txt",
    "*.js",
    "*.coffee",
    "test",
    "src",
    "lib"
  ],
  "keywords": [
    "git",
    "github",
    "contributor",
    "contributors",
    "get",
    "process",
    "update",
    "lazy",
    "automatic"
  ],
  "dependencies": {
