    "map-limit": "0.0.1",
    "minimist": "^1.1.1",
    "octonode": "^0.6.18",
    "read-file": "^0.1.2",
    "userhome": "^1.0.0"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "grunt": "~0.4.5",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-coffee": "~0.12.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-mocha-test": "~0.12.6",
    "grunt-newer": "~0.8.0",
    "grunt-release": "~0.9.0",
    "mocha": "~2.1.0",
    "pre-commit": "~1.0.2",
    "rimraf": "~2.2.8",
    "shasum": "1.0.0",
    "sinon": "~1.12.2",
    "sinon-chai": "~2.6.0"
  },
  "scripts": {
    "test": "grunt test"
